      "datasource": "Prometheus",
      "title": "API Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m])) by (handler)",
          "legendFormat": "{{handler}}"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "title": "95th Percentile Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_requests_latency_seconds_bucket[5m])) by (le, handler))",
          "legendFormat": "{{handler}}"
        }
      ],
      "unit": "s"
    },
    {
        "datasource": "Prometheus",
        "title": "HTTP Status Codes",
        "type": "stat",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "sum(increase(http_requests_total[5m])) by (status_code)",
            "legendFormat": "Status {{status_code}}"
          }
        ]
      }
